{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": 10101,
      "protocol": "trojan",
      "settings": {
        "clients": [
          {
            "password": "example",
            "email": "example@example.com"
          }
        ],
        "fallbacks": [
          {
            "dest": "www.google.com:443"
          }
        ],
        "websocket": {
          "enabled": true,
          "path": "/ws"
        }
      },
      "streamSettings": {
        "network": "tcp",
        "tcpSettings": {
          "header": {
            "type": "none"
          }
        },
        "tlsSettings": {
          "alpn": ["http/1.1"],
          "certificates": [
            {
              "certificateFile": "/path/to/certificate.crt",
              "keyFile": "/path/to/private.key"
            }
          ]
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom"
    }
  ]
}
